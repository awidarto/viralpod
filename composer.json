{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.0.*",
		"navruzm/lmongo": "*",
		"navruzm/laravel-mongo-auth": "*",
		"navruzm/laravel-mongo-validation": "dev-master",
		"way/generators": "1.0.*@dev",
		"davejamesmiller/laravel-breadcrumbs": "dev-master",
		"laravel/curl": "dev-master",
		"jasonlewis/basset": "dev-master",
		"atticmedia/anvard": "dev-master",
		"noherczeg/breadcrumb": "dev-master",
		"anahkiasen/former": "dev-agnostic",
        "sybio/image-workshop": "*",
        "intervention/image": "dev-master",
        "intervention/imagecache": "dev-master",
        "intervention/helper": "dev-master",
        "intervention/validation": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
